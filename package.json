{
    "name": "character-sheet",
    "version": "1.5.17",
    "private": true,
    "type": "module",
    "author": "Ivan Sapeha",
    "homepage": "https://ivan-sapeha.github.io/character-sheet/",
    "scripts": {
        "build": "npm run preprocess-images && npm run prepare-assets && tsc && vite build && npm run add-prefetch",
        "dev": "npm run preprocess-images && npm run prepare-assets && vite --host",
        "prepare-assets": "ts-node --esm scripts/get-default-images.ts",
        "preprocess-images": "ts-node --esm scripts/prepocess-images.ts",
        "add-prefetch": "ts-node --esm scripts/add-prefetch.ts",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx src --fix",
        "preview": "vite preview --host",
        "predeploy": "npm run build",
        "deploy": "gh-pages -d dist"
    },
    "dependencies": {
        "@msgpack/msgpack": "^3.0.0-beta2",
        "@types/react-virtualized": "^9.22.0",
        "axios": "^1.7.7",
        "classnames": "^2.3.2",
        "html2canvas": "^1.4.1",
        "nanoid": "^5.0.7",
        "react": "^18",
        "react-device-detect": "^2.2.3",
        "react-dom": "^18",
        "react-icons": "^5.3.0",
        "react-indexed-db-hook": "^1.0.14",
        "react-snow-overlay": "1.0.7",
        "react-virtualized": "^9.22.5",
        "usehooks-ts": "^3.1.0"
    },
    "devDependencies": {
        "@trivago/prettier-plugin-sort-imports": "^4.3.0",
        "@types/less": "^3.0.6",
        "@types/node": "^20.10.1",
        "@types/react": "^18.3.12",
        "@types/react-dom": "^18.2.15",
        "@types/sharp": "^0.32.0",
        "@types/wicg-file-system-access": "^2023.10.5",
        "@typescript-eslint/eslint-plugin": "^6.10.0",
        "@typescript-eslint/parser": "^6.10.0",
        "@vitejs/plugin-react": "^4.2.0",
        "autoprefixer": "^10.4.16",
        "eslint": "^8.53.0",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-plugin-import": "^2.29.0",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.4",
        "gh-pages": "^6.2.0",
        "less": "^4.2.0",
        "postcss": "^8.4.31",
        "prettier": "3.1.0",
        "sharp": "^0.33.5",
        "tailwindcss": "^3.3.5",
        "ts-node": "^10.9.2",
        "typescript": "^5",
        "vite": "^6",
        "vite-plugin-pwa": "^0.21.1"
    }
}
